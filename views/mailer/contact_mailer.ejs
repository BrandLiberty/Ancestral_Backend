<div>
    <%if(user.resent){%>
        <h3>
            User Contacted Again
        </h3>
        <%}else{%>
            <h3>New Contact Added</h3>
            <%}%>
    <div>
        <p>
            First Name : <%= user.firstName%>
        </p>
        <p>
            Last Name : <%= user.lastName%>
        </p>
        <p>
            Email : <%= user.email%>
        </p>
        
        <p>
            Phone : <%= user.phone%>
        </p>
        <% if(user.message){%>
            <p>
                Message : <%= user.message%>
            </p>
        <%}%>
    </div>
</div>